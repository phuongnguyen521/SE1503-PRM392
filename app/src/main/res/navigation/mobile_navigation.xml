    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.example.se1503_ichinsan_bookapplication.ui.dashboard.CartFragment"
        android:label="@string/title_cart"
        tools:layout="@layout/fragment_cart" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.se1503_ichinsan_bookapplication.ui.notifications.UserFragment"
